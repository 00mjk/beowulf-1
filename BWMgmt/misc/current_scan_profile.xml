		</test_module>
		<test_module>
			<module_number>1006</module_number>
			<module_name>Backup file enumerator</module_name>
			<module_enabled>false</module_enabled>
		</test_module>
	</test_modules>

	<!-- End Test Module Settings -->
</profile>

