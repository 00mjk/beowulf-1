<web-app id="BeowulfExecutorSystem">
	<display-name>Beowulf Executor System</display-name>
	<context-param>
		<param-name>bw-common-conf-filename</param-name>
		<param-value>webapp/WEB-INF/bw-common.conf</param-value>
	</context-param>
	<context-param>
		<param-name>queue-conf-filename</param-name>
		<param-value>queues.conf</param-value>
	</context-param>

	<listener>
		<listener-class>com.nvarghese.beowulf.sfe.SfeQueueManager
		</listener-class>
	</listener>
</web-app>